using System;

namespace ConcatenateData
{
    class Program
    {
        static void Main()
        {
            int dog = int.Parse(Console.ReadLine());
            int otherdog = int.Parse(Console.ReadLine());

            double cost = dog * 2.50 + otherdog * 4;

            Console.WriteLine($"{cost} lv.");

        }
    }
}
